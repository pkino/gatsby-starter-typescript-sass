    "react-dom": "^16.13.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
